{
    "configurations": {
        "C-family Launch": {
            "adapter": "CodeLLDB",
            "filetypes": ["cpp", "c"],
            "configuration": {
                "request": "launch",
                "stopOnEntry": true,
                "console": "integratedTerminal",
                "cwd": "${workspaceRoot}",
                "program": "${workspaceRoot}/a-g.out"
            },
            "breakpoints": {
                "exception": {
                    "cpp_throw": "Y",
                    "cpp_catch": "N"
                }
            }
        }
    }
}
